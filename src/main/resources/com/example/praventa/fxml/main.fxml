<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.praventa.controller.MainController">
    <children>
        <HBox prefWidth="600.0" prefHeight="400.0">
            <children>
                <VBox fx:id="sidebar" spacing="10.0" prefWidth="150.0" style="-fx-background-color: #2c3e50;" alignment="TOP_CENTER">
                    <children>
                        <Button fx:id="btnHome" text="Home" maxWidth="Infinity" styleClass="sidebar-button"/>
                        <Button fx:id="btnAbout" text="About" maxWidth="Infinity" styleClass="sidebar-button"/>
                    </children>
                </VBox>
                <AnchorPane fx:id="contentPane" prefWidth="450.0"/>
            </children>
        </HBox>
    </children>
</AnchorPane>
